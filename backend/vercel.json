{
  "version": 2,
  "builds": [
    {
      "src": "./api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/emails",
      "dest": "/controllers/emailController.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/contents",
      "dest": "/controllers/contentController.js",
      "methods": ["GET", "POST", "UPDATE"]
    }
  ]
}
